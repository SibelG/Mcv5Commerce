@using OnlineECommerceApp.Models
@model IEnumerable<ClassGroup2>

<tbody>
    @{int rankNo = 0;}
    @foreach (var x in Model)
    {
        <tr>
            <td>@{ rankNo++;}@rankNo</td>
            <td>@x.DepartmanName</td>
            <td>
                <div class="progress progress-xs">
                    <div class="progress-bar progress-bar-danger" style="width: 55%"></div>
                </div>
            </td>
            <td><span class="badge bg-danger">@x.Count</span></td>
        </tr>
    }

</tbody>

