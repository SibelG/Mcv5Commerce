@model IEnumerable<OnlineECommerceApp.Models.CargoDetail>

@{
    ViewBag.Title = "CargoDetails";
    Layout = "~/Views/Shared/_Home.cshtml";
}




<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.TrackCode)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Personel)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.Receiver)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Date)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>

            <td>
                @Html.DisplayFor(modelItem => item.TrackCode)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Personel)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.Receiver)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
               
                @Html.ActionLink("CargoTracking", "CargoTracking", "Home", new { id = item.CargoId }, new { @class = "btn btn-warning" })
                
            </td>
        </tr>
    }

</table>


